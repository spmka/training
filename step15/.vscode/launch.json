  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Client",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/client"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Server",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
